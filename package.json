{
  "name": "trello-clone",
  "version": "0.1.0",
  "description": "A Trello clone built with React",
  "main": "dist/server.js",
  "scripts": {
    "build": "webpack-dashboard -- webpack --watch",
    "build:prod": "webpack -p",
    "serve": "nodemon dist/server.js",
    "serve:prod": "NODE_ENV=production node dist/server.js",
    "analyze-bundle": "webpack -p --profile --json > stats.json && webpack-bundle-analyzer stats.json dist/public -s gzip",
    "lint": "eslint --ext .js,.jsx src/",
    "format": "prettier --write '**/*.{js,jsx,css,scss,json,md}'",
    "flow": "flow"
  },
  "author": "Markus Englund <markus.s.englund@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.17.1",
    "compression": "^1.7.1",
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "helmet": "^3.10.0",
    "mongodb": "^3.0.2",
    "morgan": "^1.9.0",
    "normalizr": "^3.2.4",
    "passport": "^0.4.0",
    "passport-twitter": "^1.0.4",
    "react": "^16.2.0",
    "react-beautiful-dnd": "^4.0.1",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-icons": "^2.2.7",
    "react-onclickoutside": "^6.7.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-textarea-autosize": "^5.2.1",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "serve-favicon": "^2.4.5",
    "shortid": "^2.2.8",
    "slugify": "^1.2.9"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.39",
    "@babel/preset-env": "^7.0.0-beta.39",
    "@babel/preset-flow": "^7.0.0-beta.39",
    "@babel/preset-react": "^7.0.0-beta.39",
    "@babel/preset-stage-2": "^7.0.0-beta.39",
    "autoprefixer": "^7.2.5",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^8.0.0-beta.0",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.9",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.41.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "flow-bin": "^0.63.1",
    "ignore-loader": "^0.1.2",
    "image-webpack-loader": "^4.0.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.14.12",
    "postcss-loader": "^2.1.0",
    "prettier": "^1.10.2",
    "sass-loader": "^6.0.6",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
